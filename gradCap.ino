#include <RGBmatrixPanel.h>
#define CLK  8
#define OE   9
#define LAT 10
#define A   A0
#define B   A1
#define C   A2
#define D   A3

RGBmatrixPanel matrix(A, B, C, D, CLK, LAT, OE, false);

void setup() {
  matrix.begin();
}

void loop() {
  delay(1000);
  matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
  matrix.setCursor(1, 0);
  matrix.setTextColor(matrix.Color333(7,7,7));
  matrix.println("GRADU");
  matrix.setCursor(1, 9);
  matrix.println("ATING");
  
  int width=0;
  int i=0;
  int percent=0;
  int significance=0;
  int previous=0;
  while(true){
    if(width==0){
      percent = 0;
    }else{
      percent = round(((float)width /  (float)32) * 100.0);  
    }
    
    significance = percent - previous;
    significance = abs(significance);
    if(significance<=9){
      delay(random(350, 450));
    }else if(significance>9 && significance<=13){
      delay(random(2800, 3200));
    }else{
      delay(random(5000, 6000));
    }

    matrix.fillRect(0, 18, 32, 20, matrix.Color333(0, 0, 0));
    matrix.fillRect(0, 18, width, 5, matrix.Color333(0, 7, 0));
    matrix.setCursor(8, 24);
    matrix.setTextColor(matrix.Color333(7,7,7));
    if (percent>=100){
      matrix.print("99");      
    }else{
      if(percent<10){
        matrix.setCursor(11, 24);
      }
      matrix.print(percent);
    }
    matrix.print("%");
    
    if(width>=32){
      delay(3000);
      matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
      delay(3000);
      //new animation
      for(int i=0; i<49; i++){
        mario();
        delay(500);
        matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
        coin();
        mario2();
        delay(500);
        matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
        if(i%10==0 && i!=0){
          matrix.setCursor(0, 8);
          matrix.print("LEVEL");
          matrix.setCursor(6, 16);
          matrix.print("UP!");
          delay(800);
          matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
        }
      }
   
      matrix.setCursor(5, 8);
      matrix.print("GAME");
      matrix.setCursor(5, 16);
      matrix.print("OVER");
      delay(4000);
      
      //end reset
      matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
      delay(5000);
      PressStart2();
      for(int i=0; i<=15; i++){
        matrix.fillRect(0, 25, 32, 7, matrix.Color333(0, 0, 0));
        delay(800);
        matrix.setCursor(2, 25);
        matrix.println("START");
        delay(800);
      }
      matrix.fillRect(0, 0, 32, 32, matrix.Color333(0, 0, 0));
      matrix.setCursor(1, 0);
      matrix.setTextColor(matrix.Color333(7,7,7));
      matrix.println("GRADU");
      matrix.setCursor(1, 9);
      matrix.println("ATING");
    }
    //reset 
    if(width >= 32){
      width=0;
      i=0;
      previous=0;
    }else{
      int myRand = random(1, 10);
      width=width+myRand; // either +1 - +5
      i=i+1;
    }
    previous=percent;
  }
}

